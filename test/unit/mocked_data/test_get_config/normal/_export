/interface bridge
add comment=BLAH mtu=1520 name=loop1
/interface ethernet
set [ find default-name=ether1 ] comment="VM Network"
set [ find default-name=ether2 ] comment=VMPG5 mtu=9000
/ip neighbor discovery
set loop1 comment=BLAH
/queue interface
set ether1 queue=ethernet-default
set ether2 queue=ethernet-default
/ip neighbor discovery
set ether1 comment="VM Network"
set ether2 comment=VMPG5
/routing bgp instance
set default router-id=10.64.0.255
add as=5388 name=test router-id=10.66.0.255 routing-table=test
add as=123 name=test2 router-id=10.67.0.255 routing-table=test
/routing ospf instance
set [ find default=yes ] router-id=10.64.0.255
/snmp community
add addresses=10.11.12.0/25,10.12.13.0/24 name=testcomm read-access=no
add addresses=0.0.0.0/0 name=banana_ro
/interface bridge port
add bridge=loop1 interface=ether2
/ip address
add address=192.168.144.222/24 interface=ether1 network=192.168.144.0
add address=10.192.0.5/24 interface=ether2 network=10.192.0.0
add address=10.64.0.255 interface=loop1 network=10.64.0.255
/ip dhcp-client
add dhcp-options=hostname,clientid interface=ether1
/ip dns
set servers=192.168.144.1
/ip route
add disabled=yes distance=1 dst-address=1.0.4.0/24 gateway=1.0.5.1 \
    routing-mark=test
add distance=1 gateway=192.168.144.1
add distance=100 dst-address=1.0.4.0/24 gateway=192.168.144.1
/ip service
set telnet disabled=yes
set ftp disabled=yes
set www disabled=yes
/ip ssh
set always-allow-password-login=yes strong-crypto=yes
/routing bgp network
add network=172.16.255.0/24 synchronize=no
/routing bgp peer
add name=vEOS-1 remote-address=192.168.144.220
add address-families=ip,ipv6 in-filter=bgp-in multihop=yes name=peer2 \
    remote-address=192.168.144.249 remote-as=5388
/routing filter
add action=accept bgp-communities=!123:321 chain=bgp-in
/routing ospf interface
add passive=yes
add interface=ether2
/routing ospf network
add area=backbone network=10.192.0.0/24
/snmp
set contact="Matt Ryan <inetuid@gmail.com>" enabled=yes engine-id=BLOB621 \
    location=Earth
/system clock
set time-zone-autodetect=no time-zone-name=GB
/system ntp client
set enabled=yes server-dns-names=\
    time.aa.net.uk,time.google.com,uk.pool.ntp.org
